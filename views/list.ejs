<%- include("header") -%>

  <div class="box" id="heading">

    <h1>It is <%= listTitle %></h1>

  </div>

  <div class="box">

    <% for(var i = 0; i < newListItems.length ; i++ ){%>
    <div class="item">
      <input type="checkbox">
      <p><%= newListItems[i] %> </p>
    </div>

    <% } %>

     <form class="item" action="/" method="post">    <!--    turnig off the suggesion -->
      <input type="text" name="newItem" placeholder="New Item" autocomplete="off">
      <button type="submit" name="list" value=<%= listTitle %>>+</button>
    </form>

  </div>
<%- include("footer") -%>
